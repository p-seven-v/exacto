{
    "name": "p7v/exacto",
    "description": "PHP Cache library",
    "type": "library",
    "require": {
        "php": "^7.0.4",
        "psr/simple-cache": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "P7v\\Exacto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "P7v\\Exacto\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
